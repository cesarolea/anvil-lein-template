{:main {{name}}.core
 {{#lambda}}
 :target :bundle
 :bundle-cmd {:none ["npx" "webpack" "--mode=development" :output-to
                     "--output-path" :final-output-dir
                     "--output-filename" :final-output-filename]
              :default ["npx" "webpack" "--mode=production" :output-to
                        "--output-path" :final-output-dir
		                    "--output-filename" :final-output-filename]}
 {{/lambda}}}
